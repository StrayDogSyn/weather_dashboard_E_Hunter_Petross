# Weather Dashboard Environment Configuration
# Copy this file to .env and fill in your API key

# OpenWeatherMap API Key (get free key at https://openweathermap.org/api)
OPENWEATHER_API_KEY=your_api_key_here

# AI Poetry Generation (Optional - for enhanced creative content)
# Get API key at https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
AI_MODEL=gpt-3.5-turbo
AI_MAX_TOKENS=150
AI_TEMPERATURE=0.8

# Optional Configuration
WEATHER_DEFAULT_CITY=New York
WEATHER_CACHE_DURATION=300
WEATHER_LOG_LEVEL=INFO
